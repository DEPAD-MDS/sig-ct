{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
            
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.py"
        }
    ],
        "build": {
        "env": {
            "PYTHON_VERSION": "3.13"
        }
    }
}